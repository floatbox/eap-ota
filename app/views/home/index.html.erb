    <meta http-equiv="X-UA-Compatible" content="IE=EmulateIE7">

    <title>Eviterra / Евитерра</title>
    <link rel="favicon icon" type="image/x-icon" href="/favicon.ico">


    <!-- [production] Все стили, импортируемые этими файлами, нужно объединить с сохранением порядка, а затем упаковать -->
    <link rel="stylesheet" type="text/css" href="css/main.css">

    <!-- [production] подключать jQuery и jQueryUI с google.com или с jquery.com -->
    <script type="text/javascript" src="js/lib/jquery-1.4.2.min.js"></script>
    <script type="text/javascript" src="js/lib/jquery-ui-1.8.min.js"></script>

    <!-- API Я.Карт -->
