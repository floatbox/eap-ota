<% @data.each do |day| %>

  <% if !day[:day_data].blank? %>
  <div style='float: left; width: 200px'>
    <h3><%= day[:title]%></h3>
    <% day[:day_data].each do |dest| %>
      <% if !dest['search'].blank? %>
        <%= dest['from'] %> <%= display_rt_arrow(dest['rt']) %> <%= dest['to'] %>
        ; <%= dest['search']['api']['total'] %>
        <% if !dest['enter'].blank? %>
        ; <%= dest['enter']['api']['total'] %>
        ; <%= (dest['enter']['api']['total'].to_f / dest['search']['api']['total'] * 100).round(2) %>%
        <% else %>
        ; - ; -
        <% end %>
      <br>
      <% end %>
    <% end %>
  </div>
  <% end %>
  
<% end %>