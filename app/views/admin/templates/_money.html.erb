<li id=<%= attribute%>>
  <label for="<%=attribute %>"> <%= Ticket.human_attribute_name attribute %> <small> [<%=attribute %>] </small> </label>
  <input class="text" id="<%=attribute %>]" include_blank="true"
    name=ticket[<%=attribute %>] size="30" type="text"
    value="<%= form.object.send(attribute) ?
      form.object.send(attribute) : '0.0 RUB' %>">
</li>

